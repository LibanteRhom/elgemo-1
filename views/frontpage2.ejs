<title>Frontpage | Elgemo</title>
    
    <%- include('include/header.ejs') %> <!-- GETS THE HEADER -->

    <div class="top-content">
        <h3>Browse All Movies</h3>
    </div>
    <main id="main">
    <% for (var i = 0 ; i < movie.length ; i++) { %>
        <div class="movie">

            <a href="/frontpage/<%= movie[i].id %>">
                <img src="<% movie[i].titleText.text %>" alt="<%= movie[i].titleText.text %>"/>
            </a>
                <div class="movie-info">
                <h3><%= movie[i].titleText.text %></h3>
            </div>
             <div class="overview">
                    <a href="/frontpage/<%= movie[i].id %>">
                        <h3>Overview:</h3>
                    </a>
            <!-- <%= movie[i].description %> -->
                <button>
                    save
                </button>
            </div>
        </div>

        <% } %>

   </main>

   <script src="js/frontpagejs.js"></script>
</body>
</html>